<Page x:Class="Onecut.View.SelectTakePic"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Onecut.View"
      mc:Ignorable="d" 
      d:DesignHeight="1080" d:DesignWidth="1920"
      Title="SelectTakePic" Background="White">

    <Grid Loaded="Grid_Loaded">
        <Image x:Name="Backgroundimg"/>
        <Image x:Name="TogetherVersion" Width="500" Height="400" Margin="1007,382,413,298" IsEnabled="True" MouseLeftButtonDown="TogetherVersion_MouseLeftButtonDown"/>
        <Image x:Name="SoloVersion" Width="500" Height="400" Margin="413,382,1007,298" MouseLeftButtonDown="SoloVersion_MouseLeftButtonDown"/>
        <Image x:Name="BackImg" Width="213" Height="73" Stretch="Fill" Margin="78,928,1630,80" MouseLeftButtonDown="BackImg_MouseLeftButtonDown"/>
        <Image x:Name="NextImg" Width="213" Height="73" Stretch="Fill" Margin="1630,928,78,80" MouseLeftButtonDown="NextImg_MouseLeftButtonDown" IsEnabled="False"/>
        <TextBlock x:Name="Timer" HorizontalAlignment="Center" FontSize="55" VerticalAlignment="Top" Width="200" Height="100" Margin="1720,130,0,0" FontFamily="Arial" FontWeight="Bold" TextAlignment="Center"/>
    </Grid>
</Page>
